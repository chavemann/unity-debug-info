{
	"metadata": [
		{
			"src": [
				{
					"src": "../",
					"files": [
						"C.DebugInfo.Runtime.csproj"
					]
				}
			],
			"dest": "api",
			"filter": "filter.yml",
			"includePrivateMembers": false,
			"disableGitFeatures": false,
			"disableDefaultFilter": false,
			"noRestore": false,
			"namespaceLayout": "flattened",
			"memberLayout": "samePage",
			"allowCompilationErrors": false
		}
	],
	"build": {
		"globalMetadata": {
			"_appTitle": "DebugInfo Documentation",
			"_appFaviconPath": "favicon.svg",
			"_appLogoPath": "logo.svg",
			"_appFooter": "DebugInfo Documentation",
			"_enableSearch": true,
			"_disableContribution": true,
			"_disableTocCollapse": false,
			
			"pdfFileName": "DebugInfo.pdf",
			"pdfTocPage": true
		},
		"fileMetadata": {
			"pdf": {
				"pdf/toc.yml": true
			}
		},
		"content": [
			{
				"//": "For whatever reason including this breaks the left TOC, so be sure to disable it when generating the site normally.",
				"fi les": ["pdf/toc.yml"]
			},
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"toc.yml",
					"*.md"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**",
					"favicon.svg",
					"logo.svg"
				]
			}
		],
		"xref": [
			"https://docs.unity3d.com/ScriptReference/xrefmap.yml"
		],
		"output": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default",
			"modern",
			"templates/modern-overrides"
		],
		"postProcessors": [],
		"keepFileLink": false,
		"disableGitFeatures": false
	}
}
